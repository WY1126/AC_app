.container_a{
  width: 100%;
  height: 3000rpx;
  background-color: #f0f0f0;
}
.head{
  width: 100%;
  height: 100rpx;
  background-color: #0F87E3;
}
.liukong{
  width: 100%;
  height: 12rpx;
  background-color: #f0f0f0;
}
.grid_a{
  width: 650rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  margin: 0 auto;
}
.grid_a_item{
  margin-top: 30rpx;
  width: 130rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.grid_a_item text{
  font-size: 12px;
  color: #555555;
}
.grid_a image{
  width: 70rpx;
  height: 70rpx;
  margin-bottom: 24rpx;
}